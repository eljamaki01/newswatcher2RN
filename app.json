{
  "expo": {
    "name": "NewsWatcher",
    "description": "News serving application",
    "slug": "NewsWatcher",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.blueskyproductions.newswatcherRN"
    },
    "android": {
      "versionCode": 3,
      "package": "com.blueskyproductions.newswatcherRN",
      "permissions": ["WAKE_LOCK"]
    } 
  }
}
